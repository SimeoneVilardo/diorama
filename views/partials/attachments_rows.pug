each file in files
    .row.no-padding(id="file-row-" + file.fileId style="padding-top: 25px;")
        .col-md-2
            button.btn.btn-danger.btn-remove-attachment(value=file.fileId.toString()) Rimuovi
        .col-md-3
            label #{file.fileName}
        .col-md-7
            .progress
                .progress-bar.progress-bar-striped.active(id="progress-bar-" + file.fileId role='progressbar' aria-valuenow='0' aria-valuemin='0' aria-valuemax='100' style='width:0%')